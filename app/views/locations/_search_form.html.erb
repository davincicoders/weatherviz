<%= form_tag({controller: "locations", action: "index"}, method: :get) do %>

    <p>
      <%= label_tag :search, "Search for a city name:" %>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil %>
    </p>
<% end %>