<% provide(:title, 'All users') %>
</div>
<div class="jumbotron">
  <div class="container">
    <div class="row">
      <div class="span6">
        <%- model_class = User -%>
        <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
      </div>
    </div>
  </div>
</div>
<div class="main-content">
  <div class="container">

    <div class="row">
      <div class="about span12" >

        <%= will_paginate %>
        <br>

        <ul class="users">
          <%= render @users %>
        </ul>

        <br>
        <%= will_paginate %>

      </div>
    </div>
  </div>
</div>



